{{!< default}}

{{#post}}

  <article class="page-credits {{post_class}}">
    {{> material-cover}}
    <div class="post-card animated fade-in">
        <div class="post-container">
            <div class="post-header">
                <h1 id="post-title">
                    {{{title}}}
                </h1>
            </div>
            <div class="post-content">
                {{content}}
            </div>
        </div>
    </div>
  </article>

{{/post}}
